<a href="/register">Registrarse</a>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(function(user){ %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
        </tr>
        <% }); %>
    </tbody>
</table>